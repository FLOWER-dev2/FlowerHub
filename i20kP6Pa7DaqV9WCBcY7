    Title = "Kill Aura",
    Description = "Ï£ºÎ≥Ä ÏÇ¨ÎûåÎì§ÏùÑ Ï£ΩÏûÖÎãàÎã§.",
    Callback = function()
        Fluent:Notify({
            Title = "Notification",
            Content = "ÏÑ±Í≥µÏ†ÅÏúºÎ°ú Ïã§ÌñâÎêòÏóàÏäµÎãàÎã§.",
            SubContent = "KÎ•º ÎàåÎü¨ ÌÜ†Í∏Ä",
            Duration = 5
        })
