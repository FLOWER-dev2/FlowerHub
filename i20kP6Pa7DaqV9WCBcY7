    if autoEquipEnabled then
        local XDF = workspace:FindFirstChild("34efw@#$%R#$@R%wasfdqw#$t??")
        local FDF = game:GetService("Players").LocalPlayer.Character
        local humanoid = FDF and FDF:FindFirstChild("Humanoid")
